<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="popUpPane" prefHeight="148.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.EarthquakePopUpWindowController">
   <children>
      <Label layoutX="15.0" layoutY="24.0" text="Please enter a file path:">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <TextField fx:id="fileNameTextField" layoutX="169.0" layoutY="21.0" prefHeight="26.0" prefWidth="216.0" />
      <Label fx:id="warningLabel" layoutX="14.0" layoutY="65.0" textFill="RED" />
      <Button fx:id="writeToFileButton" layoutX="62.0" layoutY="108.0" mnemonicParsing="false" onAction="#writeToFileButtonClicked" text="Write to File" />
      <Button fx:id="cancelButton" layoutX="286.0" layoutY="108.0" mnemonicParsing="false" onAction="#cancelButtonClicked" text="Cancel" />
   </children>
</AnchorPane>